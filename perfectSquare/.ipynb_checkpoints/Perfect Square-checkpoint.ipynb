   "source": [
    "# Code to find the next perfect square after the input one (n)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "import math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "n = 25"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "perfSQ = math.sqrt(n)\n",
    "\n",
    "perfSQ = perfSQ + 1\n",
    "res = perfSQ*perfSQ"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Next Perfect Square 36.0\n"
     ]
    }
   ],
   "source": [
    "print (\"Next Perfect Square\", res)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
